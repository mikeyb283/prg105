# 5ml = 1tsp
def convert(milliliters):
    teaspoons = round((float(milliliters) * .202884))
    return teaspoons

print ('Easily convert your baking ingredients from milliliters to teaspoons!')
milliliters = raw_input('How many milliliters does your recipe require?\n')
teaspoons = convert (milliliters)
print ("For" + ' '+ milliliters + 'ml ' + "use" + ' ' + str(teaspoons) + "tsp")
